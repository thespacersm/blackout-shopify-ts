{% if section.settings.select_hds == "header_v5" %}
{% if template contains 'index' %}
<header id="header" class="header-v5-h5 js_height_hd jsheader_sticky d-none d-xl-block {% if section.settings.hd_home_enable_transparents %}header-absolute {% endif %}">
  {% if settings.enable_topbar_v5 %}
  <div id="bwp-topbar" class="topbar-v1" {% if settings.bg_topbar_v5 !=blank %}style="background :{{settings.bg_topbar_v5}}" {% endif %}>
    <div class="topbar-inner">
      <div class="container container-v1">
        <div class="row">
          <div class="col-lg-6 col-md-6 col-sm-6 topbar-left {% if settings.enable_rtl_mode %} text-lg-right {% else %}text-lg-left{% endif %}">
            <div class="list-icon">
              {% if settings.icon_sc_tb5_1 !=blank %}
              <a href="{{settings.url_sc_tb5_1}}" title="" target="_blank" class="social-item"><i class="{{settings.icon_sc_tb5_1}}"></i></a>
              {% endif %}
              {% if settings.icon_sc_tb5_2 !=blank %}
              <a href="{{settings.url_sc_tb5_2}}" title="" target="_blank" class="social-item"><i class="{{settings.icon_sc_tb5_2}}"></i></a>
              {% endif %}
              {% if settings.icon_sc_tb5_3 !=blank %}
              <a href="{{settings.url_sc_tb5_3}}" title="" target="_blank" class="social-item"><i class="{{settings.icon_sc_tb5_3}}"></i></a>
              {% endif %}
              {% if settings.icon_sc_tb5_4 !=blank %}
              <a href="{{settings.url_sc_tb5_4}}" title="" target="_blank" class="social-item"><i class="{{settings.icon_sc_tb5_4}}"></i></a>
              {% endif %}
            </div>
          </div>
          <div class="col-lg-6 col-md-6 col-sm-6 topbar-right {% if settings.enable_rtl_mode %}text-lg-left {% else %}text-lg-right{% endif %}">
            <div class="menu-currency-language">
              {% if settings.enable_currency_t5 %}
              <div class="menu-currency-wrap">
                {% include 'currency' %}
              </div>
              {% endif %}
              {% if settings.enable_multi_lang_t5 %}
              <div class="menu-language-wrap">
                {% include 'change-language' %}
              </div>
              {% endif %}
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  {% endif %}
  
  
  {% include 'eveland-horizon-menu' %}
  <div class="header-wrapper">
    <div class="container container-v1">
      <div class="row align-items-center">
        <div class="col-lg-5">
          <div class="menu {% if settings.enable_rtl_mode %}right {% else %}left{% endif %}">
            <div class="model_menu js-model_menu">
              <span class="model-menu-icon">
                <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <path d="M21 7.75H3C2.59 7.75 2.25 7.41 2.25 7C2.25 6.59 2.59 6.25 3 6.25H21C21.41 6.25 21.75 6.59 21.75 7C21.75 7.41 21.41 7.75 21 7.75Z" fill="#30343A"/>
                  <path d="M21 12.75H3C2.59 12.75 2.25 12.41 2.25 12C2.25 11.59 2.59 11.25 3 11.25H21C21.41 11.25 21.75 11.59 21.75 12C21.75 12.41 21.41 12.75 21 12.75Z" fill="#30343A"/>
                  <path d="M21 17.75H3C2.59 17.75 2.25 17.41 2.25 17C2.25 16.59 2.59 16.25 3 16.25H21C21.41 16.25 21.75 16.59 21.75 17C21.75 17.41 21.41 17.75 21 17.75Z" fill="#30343A"/>
                </svg>
              </span>
            </div>
          </div>
        </div>
        <div class="col-lg-2 text-center">
          <div class="logo">
            <div class="center logo delay05">           
              {% if section.settings.hd_home_logo_svg !=blank %}
              <div class="logosvg m-auto" style="max-width:{{section.settings.hd_home_logo_maxwidth}}px;">
                <a class="logourl" href="{{shop.url}}">{{section.settings.hd_home_logo_svg}}</a>
              </div>
              {% else %}            
              <a href="{{shop.url}}">
                {% if section.settings.hd_home_logo != blank %}
                <img src="{{section.settings.hd_home_logo|img_url:'master'}}" width="{{section.settings.hd_home_logo_maxwidth}}" alt="{{shop.name}}">
                {% else %}
                <img src="//via.placeholder.com/160x46" width="{{section.settings.hd_home_logo_maxwidth}}" alt="{{shop.name}}">
                {% endif %}
              </a>
              {% endif %}          
            </div>
          </div>
        </div>      
        <div class="col-lg-5 currencies-login">
          <div class="cart-login-search align-items-center"> 
            <ul class="list-inline list-unstyled mb-0">
              <li class="list-inline-item mr-0">
                <a href="javascript:void(0)" class="search js-search-destop">
                  {% if section.settings.hd_icon_search_svg != blank %}
                  {{section.settings.hd_icon_search_svg}}
                  {% else %}
                  <i class="{{section.settings.hd_icon_search}}"></i>
                  {% endif %}
                </a>
              </li>
              <li class="list-inline-item mr-0">
                <a href="{% if customer %}/account {% else %}javascript:void(0){% endif %}" class="login {% if customer %}{% else %}js-call-popup-login{% endif %}">
                  {% if section.settings.hd_icon_login_svg != blank %}
                  {{section.settings.hd_icon_login_svg}}
                  {% else %}
                  <i class="{{section.settings.hd_icon_login}}"></i>
                  {% endif %}

                </a>
              </li>
              <li class="list-inline-item mr-0">
                <a href="{{section.section.settings.url_wishlist}}" class="">
                  {% if section.settings.hd_icon_wishlist_svg != blank %}
                  {{section.settings.hd_icon_wishlist_svg}}
                  {% else %}
                  <i class="{{section.settings.hd_icon_wishlist}}"></i>
                  {% endif %}

                </a>
              </li>
              <li class="list-inline-item mr-0">
                <a href="javascript:void(0)" class="cart js-call-minicart">
                  {% if section.settings.hd_icon_cart_svg != blank %}
                  {{section.settings.hd_icon_cart_svg}}
                  {% else %}
                  <i class="{{section.settings.hd_icon_cart}}"></i>
                  {% endif %}
                  <span class="count_pr_incart js-number-cart number-cart">{% if cart.item_count > 0 %} {{cart.item_count}} {% else %} 0 {% endif %}</span>
                </a>
              </li>
            </ul>

          </div>
          <div class="clearfix"></div>
        </div>
      </div>
    </div>
  </div>
</header>


<script>
  jQuery(document).ready(function($) {
    
    function hexToRgb(hex) {
      var result = /^#?([a-f\d]{2})([a-f\d]{2})([a-f\d]{2})$/i.exec(hex);
      return result ? {
        r: parseInt(result[1], 16),
        g: parseInt(result[2], 16),
        b: parseInt(result[3], 16)
      } : null;
    }
    var r = (hexToRgb("{{section.settings.hd_home_bg_header_normal}}").r);
    var g = (hexToRgb("{{section.settings.hd_home_bg_header_normal}}").g);
    var b = (hexToRgb("{{section.settings.hd_home_bg_header_normal}}").b)
    {% if section.settings.hd_home_enable_transparents %}
    {% else %}                  
    $('.header-v5-h5').css({'background':'rgba(' + r + ' ,' + g + ',' + b + ',{{section.settings.hd_bg_header_opacity}})'});
    {% endif %}
    
    {% if section.settings.enable_sticky_hd_home  %}
    function menudestopscroll5() {
      var $nav = $(".jsheader_sticky");
      $nav.removeClass('menu_scroll_v5');

      $(document).scroll(function() {

        $nav.toggleClass('menu_scroll_v5', $(this).scrollTop() > $nav.height());
        var r = (hexToRgb("{{section.settings.hd_home_bg_header_normal}}").r);
        var g = (hexToRgb("{{section.settings.hd_home_bg_header_normal}}").g);
        var b = (hexToRgb("{{section.settings.hd_home_bg_header_normal}}").b)
        $('.header-v5-h5.menu_scroll_v5').css({'background':'rgba(' + r + ' ,' + g + ',' + b + ',{{section.settings.hd_bg_header_opacity}})'});
        {% if section.settings.hd_home_enable_transparents %}                    
        if($(this).scrollTop() < $nav.height()){
          $('.header-v5-h5').css({'background':'none'}); 
            $nav.removeClass('menu_scroll_v5')
        }
        {% endif %}
      });
    } 
    menudestopscroll5();
    {% endif %}
    
    
  });
</script>

<style>
  
  #header .menu ul li .nav-link{
    color: {{section.settings.hd_home_color_text}};
  }

  #header .currencies-login .cart-login-search ul li a i{
    color: {{section.settings.hd_home_color_text}};
  }
  
  #header .currencies-login .cart-login-search ul li .cart .number-cart{
    color : {{section.settings.hd_home_color_text}};
  }
  .header-v5-h5 .model_menu{
    color : {{section.settings.hd_home_color_text}};
  }
  .header-v5-h5 .model_menu .model-menu-icon span{
    background : {{section.settings.hd_home_color_text}};
  }
  .header-v5-h5 .horizon-menu-home{
    background : {{settings.hdv5_bg_menu}};
  }
  .header-v5-h5 .horizon-menu-home .main-navigation .menu-horizon-list .nammenu{
    color : {{settings.hdv5_color_menu_title}};
  }
  .horizon-menu-home .main-navigation .menu-horizon-list .nammenu:before{
    color : {{settings.hdv5_color_menu_title}};
  }
  .horizon-menu-home .main-navigation .menu-horizon-list .toggle-menumobile i{
    color : {{settings.hdv5_color_menu_title}};
  }
  .horizon-menu-home .main-navigation .menu-horizon-list .title_menu_mb.title_only_one a{
    color : {{settings.hdv5_color_menu_title}};
  }
  .horizon-menu-home .main-navigation .menu-horizon-list .title_menu_mb.title_only_one a:hover{
    opacity : .8;
  }
  
  .horizon-menu-home .main-navigation .menu-horizon-list .title_menu_mb a{
    color : {{settings.hdv5_color_menu_title}};
  }
  
  .horizon-menu-home .main-navigation .menu-horizon-list .title_menu_mb a:hover{
    opacity : .8;
  }
  .horizon-menu-home .main-navigation .menu-horizon-list ul li a{
    color : {{settings.hdv5_color_menu_title}};
  }
  
  .horizon-menu-home .main-navigation .menu-horizon-list ul li a:hover{
    opacity : .8;
  }
  .horizon-menu-home .close-menu-horizon{
    color : {{settings.hdv5_color_menu_title}};
  }
</style>
{% endif %}
{% endif %}

